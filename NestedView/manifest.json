{
    "_version": "1.12.0",
    "sap.app":{
        "id":"app",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "titel":"{{pageTitle}}",
        "description" : "{{description}}",
        "applicationVersion" :{
        "version" :"1.0.0"
        }
    },
    "sap.ui" :{
        "technology" :"UI5",
        "deviceTypes" :{
            "desktop" : true,
            "phone": true,
            "tablet": true
        }
    },
    "sap.ui5":{
        "rootView":{
            "viewName":"app.view.Second",
            "type": "XML",
            "id" : "app"
        },
        "dependecies":{
            "minVersion":"1.93",
            "libs" :{
                "sap.m":{},
                "sap.ui.core":{}
            }
        },
        "models":{
            "i18n":{
                "type": "sap.ui.model.resource.ResourceModel",
                "settings" :{
                    "bundleName" :"app.i18n.i18n",
                    "supportedLocales" :[""],
                    "fallbackLocale" :""
                }
            }
        }
    }
}